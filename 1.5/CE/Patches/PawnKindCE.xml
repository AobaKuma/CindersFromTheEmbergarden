<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
    <Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/PawnKindDef[defName="Cinder_OutlanderElite" or defName="Cinder_HeavySoliderUnit" or defName="Cinder_SoliderUnit"]</xpath>
		<value>
		    <li Class="CombatExtended.LoadoutPropertiesExtension">
			    <primaryMagazineCount>
					<min>5</min>
					<max>8</max>
				</primaryMagazineCount>
				<sidearms>
					<li>
						<generateChance>1</generateChance>
						<sidearmMoney>
							<min>550</min>
							<max>570</max>
						</sidearmMoney>
						<weaponTags>
							<li>CinderSimpleGuns</li>
							<li>CinderPDW</li>
						</weaponTags>
						<magazineCount>
							<min>2</min>
							<max>3</max>
						</magazineCount>
					</li>
			    </sidearms>
			</li>
		</value>
	</Operation>
	
	 <Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/PawnKindDef[defName="Cinder_Grenadier" or defName="Cinder_BreakUnit"]</xpath>
		<value>
		    <li Class="CombatExtended.LoadoutPropertiesExtension">
			    <primaryMagazineCount>
					<min>10</min>
					<max>12</max>
				</primaryMagazineCount>
				<sidearms>
					<li>
						<generateChance>1</generateChance>
						<sidearmMoney>
							<min>550</min>
							<max>570</max>
						</sidearmMoney>
						<weaponTags>
							<li>CinderSimpleGuns</li>
							<li>CinderPDW</li>
						</weaponTags>
						<magazineCount>
							<min>2</min>
							<max>3</max>
						</magazineCount>
					</li>
			    </sidearms>
			</li>
		</value>
	</Operation>
	
