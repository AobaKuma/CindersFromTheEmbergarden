    <!-- Cinder_Apparel_Ghost -->
    <ThingDef ParentName="Cinder_ApparelBase">
		<defName>Cinder_Apparel_GhostUnderwear</defName>
		<label>ghost</label>
		<description>A strange hollow out skirt underwear, completely defenseless, but very capable of adorning the body.It seems that there is some strange device installed on it, and the person wearing it will enter a state of heightened libido.</description>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/Ghost/Icon</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>Cinder_TwistedPharmacology</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
		    <Steel>10</Steel>
			<Cloth>60</Cloth>
			<ComponentSpacer>1</ComponentSpacer>
			<Neutroamine>10</Neutroamine>
		</costList>
		<statBases>
			<MaxHitPoints>160</MaxHitPoints>
			<MarketValue>200</MarketValue>
			<WorkToMake>5000</WorkToMake>
			<Mass>0.5</Mass>
			<ArmorRating_Sharp>0.3</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.1</ArmorRating_Blunt>
			<ArmorRating_Heat>0.1</ArmorRating_Heat>
			<Insulation_Cold>3.4</Insulation_Cold>
			<Insulation_Heat>3</Insulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/Ghost/GhostUnderwear</wornGraphicPath>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags Inherit="false">
				<li>Cinder_Private</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
	<equippedStatOffsets>
	    <PawnBeauty>1</PawnBeauty>
        <SocialImpact>0.2</SocialImpact>
    </equippedStatOffsets>
