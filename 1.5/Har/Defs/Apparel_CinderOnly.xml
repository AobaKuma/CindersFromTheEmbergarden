<Defs>
	<!-- Cinder_Apparel_Commemorative -->
	<ThingDef ParentName="Cinder_ApparelBase">
		<defName>Cinder_Apparel_Commemorative</defName>
		<label>commemorative</label>
		<description>A gorgeous dress used to commemorate someone, decorated with some metal pieces.</description>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/Commemorative/Icon</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<Steel>20</Steel>
		</costList>
		<costStuffCount>75</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>160</MaxHitPoints>
			<MarketValue>200</MarketValue>
			<WorkToMake>5000</WorkToMake>
			<Mass>0.5</Mass>
			<ArmorRating_Sharp>0.2</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.2</ArmorRating_Blunt>
			<StuffEffectMultiplierArmor>0.20</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.36</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.40</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/Commemorative/Commemorative</wornGraphicPath>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<li>Cinder_CivilanC</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>
	
