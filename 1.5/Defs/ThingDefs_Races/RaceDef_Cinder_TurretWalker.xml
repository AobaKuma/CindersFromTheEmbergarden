<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="Cinder_BaseTurretWalker">
		<defName>Cinder_Mech_WalkerAutoCannon</defName>
		<label>Autocannon walker</label>
		<description>A large walking turret with thick armor and large caliber automatic cannon, which can be used as a small IFV.</description>
		<tools>
			<li>
				<label>Left tentacle</label>
				<labelNoLocation>tentacle</labelNoLocation>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>10.0</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
			</li>
			<li>
				<label>right tentacle</label>
				<labelNoLocation>tentacle</labelNoLocation>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>10.0</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
			</li>
			<li>
				<label>Front tentacle</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>15.5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<race>
		<intelligence>ToolUser</intelligence>
			<thinkTreeMain>Cinder_WalkerTurret</thinkTreeMain>
			<body>Cinder_TurretWalker</body>
			<baseHealthScale>3</baseHealthScale>
		</race>
		<modExtensions>

			<li Class="Embergarden.ModExtension_Draftable">
			</li>
		</modExtensions>
	</ThingDef>

	<PawnKindDef ParentName="BaseMechanoidKind">
		<defName>Cinder_Mech_WalkerAutoCannon</defName>
		<label>Autocannon walker</label>
		<race>Cinder_Mech_WalkerAutoCannon</race>
		<combatPower>650</combatPower>
		<maxPerGroup>5</maxPerGroup>
		<allowInMechClusters>false</allowInMechClusters>
		<lifeStages>
			<li MayRequire="Ludeon.RimWorld.Biotech">
				<bodyGraphicData>
					<texPath>Things/Building/WalkerAutoCannon/WalkerAutocannon</texPath>
					<shaderType>CutoutComplex</shaderType>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(1.86, 1.86)</drawSize>
					<shadowData>
						<volume>(1.5,0.35,1.4)</volume>
					</shadowData>
					<drawOffset>(0,0,-0.05)</drawOffset>
				</bodyGraphicData>
			</li>
			<li>
				<bodyGraphicData>
					<texPath>Things/Building/WalkerAutoCannon/WalkerAutocannon</texPath>
					<shaderType>CutoutComplex</shaderType>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(1.86, 1.86)</drawSize>
					<shadowData>
						<volume>(1.5,0.35,1.4)</volume>
					</shadowData>
					<drawOffset>(0,0,-0.05)</drawOffset>
				</bodyGraphicData>
			</li>
		</lifeStages>
		<techHediffsChance>1</techHediffsChance>
		<techHediffsMoney>9999~9999</techHediffsMoney>
		<abilities>
			<li>Ability_Transformation_Cannon</li>
		</abilities>
	</PawnKindDef>


