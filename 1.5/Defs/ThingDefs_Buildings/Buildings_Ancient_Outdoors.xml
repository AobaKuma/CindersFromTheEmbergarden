<Defs>
    <ThingDef ParentName="NonDeconstructibleAncientBuildingBase" Name="Cinder_AncientBuildingBase" Abstract="True">
        <graphicData>
            <graphicClass>Graphic_Multi</graphicClass>
        </graphicData>
        <rotatable>true</rotatable>
        <randomizeRotationOnSpawn>true</randomizeRotationOnSpawn>
        <passability>Impassable</passability>
		<blockWind>true</blockWind>
		<disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
    </ThingDef>

    <ThingDef ParentName="Cinder_AncientBuildingBase">
        <defName>Cinder_AncientRustedChunk_A</defName>
        <label>ancient automatic truck</label>
        <description>An ancient, broken chunk.There seems to be some useful things inside.</description>
        <graphicData>
            <texPath>Things/Building/AncientAutomaticTruck/building</texPath>
            <drawSize>(4,8)</drawSize>
            <shadowData>
                <volume>(2, 2, 5)</volume>
            </shadowData>
        </graphicData>
        <size>(2,6)</size>
		<pathCost>50</pathCost>
		<passability>PassThroughOnly</passability>
        <fillPercent>1</fillPercent>
        <statBases>
            <MaxHitPoints>720</MaxHitPoints>
        </statBases>
        <killedLeavings>
            <ChunkSlagSteel>6</ChunkSlagSteel>
            <MedicineIndustrial>2</MedicineIndustrial>
			<MealSurvivalPack>5</MealSurvivalPack>
        </killedLeavings>
    </ThingDef>
	
	

