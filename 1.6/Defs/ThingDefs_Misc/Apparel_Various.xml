	<!-- Cinder_Apparel_ScavHeavyCoat -->
	<ThingDef
	  ParentName="Cinder_ApparelBase">
		<defName>Cinder_Apparel_ScavHeavyCoat</defName>
		<label>scav heavy coat</label>
		<description>A tattered scavenger coat made of a blend of metal plates and fabric, much more durable than it looks.</description>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/ScavHeavyCoat/Icon</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>FlakArmor</researchPrerequisite>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<Cloth>90</Cloth>
			<Steel>75</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
